package blackjackpackage;

import java.util.ArrayList;
/**
 *
 * @author guest1Day
 */

//抽象クラス
abstract public class Human {
    protected ArrayList <Integer> myCards;  //フィールド
    abstract public int open();  //手札の合計値メソッド
    abstract public void setCard(ArrayList<Integer> c);  //手札に追加メソッド
    abstract public boolean checkSum();  //カードを弾くべきかメソッド
    
}

    
    
    

